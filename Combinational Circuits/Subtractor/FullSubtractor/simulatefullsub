#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x109b2d0 .scope module, "onebitfullsubTB" "onebitfullsubTB" 2 1;
 .timescale 0 0;
v0x10b5f70_0 .var "A", 0 0;
v0x10b6040_0 .var "B", 0 0;
v0x10b6110_0 .var "BorrowIn", 0 0;
v0x10b61e0_0 .net "BorrowOut", 0 0, L_0x10b6330; 1 drivers
v0x10b6260_0 .net "Diff", 0 0, v0x10b53c0_0; 1 drivers
S_0x10b5e80 .scope task, "initialize" "initialize" 2 12, 2 12, S_0x109b2d0;
 .timescale 0 0;
TD_onebitfullsubTB.initialize ;
    %set/v v0x10b5f70_0, 0, 1;
    %set/v v0x10b6040_0, 0, 1;
    %set/v v0x10b6110_0, 0, 1;
    %end;
S_0x109b3c0 .scope module, "DUT" "onebitfullsub" 2 10, 3 1, S_0x109b2d0;
 .timescale 0 0;
L_0x10b6330 .functor OR 1, v0x10b5730_0, v0x10b5320_0, C4<0>, C4<0>;
v0x10b58b0_0 .net "A", 0 0, v0x10b5f70_0; 1 drivers
v0x10b5960_0 .net "B", 0 0, v0x10b6040_0; 1 drivers
v0x10b5a10_0 .net "BorrowIn", 0 0, v0x10b6110_0; 1 drivers
v0x10b5ac0_0 .alias "BorrowOut", 0 0, v0x10b61e0_0;
v0x10b5b70_0 .alias "Diff", 0 0, v0x10b6260_0;
v0x10b5c20_0 .net "Diff1", 0 0, v0x10b57d0_0; 1 drivers
v0x10b5d30_0 .net "borrow1", 0 0, v0x10b5730_0; 1 drivers
v0x10b5db0_0 .net "borrow2", 0 0, v0x10b5320_0; 1 drivers
S_0x10b5470 .scope module, "hs1" "halfsub" 3 12, 4 1, S_0x109b3c0;
 .timescale 0 0;
v0x10b55d0_0 .alias "A", 0 0, v0x10b58b0_0;
v0x10b5690_0 .alias "B", 0 0, v0x10b5960_0;
v0x10b5730_0 .var "Borrow", 0 0;
v0x10b57d0_0 .var "Diff", 0 0;
E_0x10b5560 .event edge, v0x10b55d0_0, v0x10b5690_0;
S_0x109d960 .scope module, "hs2" "halfsub" 3 13, 4 1, S_0x109b3c0;
 .timescale 0 0;
v0x108ac00_0 .alias "A", 0 0, v0x10b5c20_0;
v0x10b5280_0 .alias "B", 0 0, v0x10b5a10_0;
v0x10b5320_0 .var "Borrow", 0 0;
v0x10b53c0_0 .var "Diff", 0 0;
E_0x1065bc0 .event edge, v0x108ac00_0, v0x10b5280_0;
    .scope S_0x10b5470;
T_1 ;
    %wait E_0x10b5560;
    %load/v 8, v0x10b5690_0, 1;
    %load/v 9, v0x10b55d0_0, 1;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_1.0, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_1.1, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_1.2, 6;
    %cmpi/u 8, 3, 2;
    %jmp/1 T_1.3, 6;
    %ix/load 0, 1, 0;
    %assign/v0 v0x10b57d0_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0x10b5730_0, 0, 0;
    %jmp T_1.5;
T_1.0 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0x10b57d0_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0x10b5730_0, 0, 0;
    %jmp T_1.5;
T_1.1 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0x10b57d0_0, 0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x10b5730_0, 0, 1;
    %jmp T_1.5;
T_1.2 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0x10b57d0_0, 0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x10b5730_0, 0, 0;
    %jmp T_1.5;
T_1.3 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0x10b57d0_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0x10b5730_0, 0, 0;
    %jmp T_1.5;
T_1.5 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x109d960;
T_2 ;
    %wait E_0x1065bc0;
    %load/v 8, v0x10b5280_0, 1;
    %load/v 9, v0x108ac00_0, 1;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_2.0, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_2.1, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_2.2, 6;
    %cmpi/u 8, 3, 2;
    %jmp/1 T_2.3, 6;
    %ix/load 0, 1, 0;
    %assign/v0 v0x10b53c0_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0x10b5320_0, 0, 0;
    %jmp T_2.5;
T_2.0 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0x10b53c0_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0x10b5320_0, 0, 0;
    %jmp T_2.5;
T_2.1 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0x10b53c0_0, 0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x10b5320_0, 0, 1;
    %jmp T_2.5;
T_2.2 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0x10b53c0_0, 0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x10b5320_0, 0, 0;
    %jmp T_2.5;
T_2.3 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0x10b53c0_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0x10b5320_0, 0, 0;
    %jmp T_2.5;
T_2.5 ;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x109b2d0;
T_3 ;
    %vpi_call 2 22 "$monitor", $time, " A = %b, B = %b, BorrowIn = %b, Diff = %b, BorrowOut = %b", v0x10b5f70_0, v0x10b6040_0, v0x10b6110_0, v0x10b6260_0, v0x10b61e0_0;
    %vpi_call 2 23 "$dumpfile", "fullsubwaveform.vcd";
    %vpi_call 2 24 "$dumpvars", 1'sb0, S_0x109b2d0;
    %end;
    .thread T_3;
    .scope S_0x109b2d0;
T_4 ;
    %fork TD_onebitfullsubTB.initialize, S_0x10b5e80;
    %join;
    %delay 5, 0;
    %set/v v0x10b5f70_0, 0, 1;
    %set/v v0x10b6040_0, 0, 1;
    %set/v v0x10b6110_0, 0, 1;
    %delay 5, 0;
    %set/v v0x10b5f70_0, 0, 1;
    %set/v v0x10b6040_0, 0, 1;
    %set/v v0x10b6110_0, 1, 1;
    %delay 5, 0;
    %set/v v0x10b5f70_0, 0, 1;
    %set/v v0x10b6040_0, 1, 1;
    %set/v v0x10b6110_0, 0, 1;
    %delay 5, 0;
    %set/v v0x10b5f70_0, 0, 1;
    %set/v v0x10b6040_0, 1, 1;
    %set/v v0x10b6110_0, 1, 1;
    %delay 5, 0;
    %set/v v0x10b5f70_0, 1, 1;
    %set/v v0x10b6040_0, 0, 1;
    %set/v v0x10b6110_0, 0, 1;
    %delay 5, 0;
    %set/v v0x10b5f70_0, 1, 1;
    %set/v v0x10b6040_0, 0, 1;
    %set/v v0x10b6110_0, 1, 1;
    %delay 5, 0;
    %set/v v0x10b5f70_0, 1, 1;
    %set/v v0x10b6040_0, 1, 1;
    %set/v v0x10b6110_0, 0, 1;
    %delay 5, 0;
    %set/v v0x10b5f70_0, 1, 1;
    %set/v v0x10b6040_0, 1, 1;
    %set/v v0x10b6110_0, 1, 1;
    %delay 5, 0;
    %vpi_call 2 38 "$finish";
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
