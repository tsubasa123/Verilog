#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xb21730 .scope module, "halfsubTB" "halfsubTB" 2 1;
 .timescale 0 0;
v0xb6a3e0_0 .var "A", 0 0;
v0xb6a4b0_0 .var "B", 0 0;
v0xb6a560_0 .net "Borrow", 0 0, v0xb6a1a0_0; 1 drivers
v0xb6a610_0 .net "Diff", 0 0, v0xb6a240_0; 1 drivers
S_0xb6a2f0 .scope task, "initialize" "initialize" 2 9, 2 9, S_0xb21730;
 .timescale 0 0;
TD_halfsubTB.initialize ;
    %set/v v0xb6a3e0_0, 0, 1;
    %set/v v0xb6a4b0_0, 0, 1;
    %end;
S_0xb21820 .scope module, "DUT" "halfsub" 2 7, 3 1, S_0xb21730;
 .timescale 0 0;
v0xb58220_0 .net "A", 0 0, v0xb6a3e0_0; 1 drivers
v0xb6a100_0 .net "B", 0 0, v0xb6a4b0_0; 1 drivers
v0xb6a1a0_0 .var "Borrow", 0 0;
v0xb6a240_0 .var "Diff", 0 0;
E_0xb5a950 .event edge, v0xb58220_0, v0xb6a100_0;
    .scope S_0xb21820;
T_1 ;
    %wait E_0xb5a950;
    %load/v 8, v0xb6a100_0, 1;
    %load/v 9, v0xb58220_0, 1;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_1.0, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_1.1, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_1.2, 6;
    %cmpi/u 8, 3, 2;
    %jmp/1 T_1.3, 6;
    %ix/load 0, 1, 0;
    %assign/v0 v0xb6a240_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0xb6a1a0_0, 0, 0;
    %jmp T_1.5;
T_1.0 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0xb6a240_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0xb6a1a0_0, 0, 0;
    %jmp T_1.5;
T_1.1 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0xb6a240_0, 0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0xb6a1a0_0, 0, 1;
    %jmp T_1.5;
T_1.2 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0xb6a240_0, 0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0xb6a1a0_0, 0, 0;
    %jmp T_1.5;
T_1.3 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0xb6a240_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0xb6a1a0_0, 0, 0;
    %jmp T_1.5;
T_1.5 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0xb21730;
T_2 ;
    %vpi_call 2 19 "$monitor", $time, " A = %B, B = %b, Diff = %b, Borrow = %b", v0xb6a3e0_0, v0xb6a4b0_0, v0xb6a610_0, v0xb6a560_0;
    %vpi_call 2 20 "$dumpfile", "halfsubwaveform.vcd";
    %vpi_call 2 21 "$dumpvars", 1'sb0, S_0xb21730;
    %end;
    .thread T_2;
    .scope S_0xb21730;
T_3 ;
    %fork TD_halfsubTB.initialize, S_0xb6a2f0;
    %join;
    %delay 10, 0;
    %set/v v0xb6a3e0_0, 0, 1;
    %set/v v0xb6a4b0_0, 0, 1;
    %delay 20, 0;
    %set/v v0xb6a3e0_0, 1, 1;
    %set/v v0xb6a4b0_0, 0, 1;
    %delay 35, 0;
    %set/v v0xb6a3e0_0, 0, 1;
    %set/v v0xb6a4b0_0, 1, 1;
    %delay 5, 0;
    %set/v v0xb6a3e0_0, 1, 1;
    %set/v v0xb6a4b0_0, 1, 1;
    %delay 5, 0;
    %vpi_call 2 31 "$finish";
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
