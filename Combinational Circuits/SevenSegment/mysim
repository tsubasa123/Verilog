#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1b4c730 .scope module, "BcdTo7segmentTB" "BcdTo7segmentTB" 2 1;
 .timescale 0 0;
v0x1b93cc0_0 .net "Out", 6 0, v0x1b82fd0_0; 1 drivers
v0x1b93d90_0 .var "bcd", 3 0;
v0x1b93e40_0 .var/i "i", 31 0;
S_0x1b4c820 .scope module, "DUT" "BcdTo7segment" 2 6, 3 1, S_0x1b4c730;
 .timescale 0 0;
v0x1b82fd0_0 .var "Out", 6 0;
v0x1b93c20_0 .net "bcd", 3 0, v0x1b93d90_0; 1 drivers
E_0x1b5ed80 .event edge, v0x1b93c20_0;
    .scope S_0x1b4c820;
T_0 ;
    %wait E_0x1b5ed80;
    %load/v 8, v0x1b93c20_0, 4;
    %cmpi/u 8, 0, 4;
    %jmp/1 T_0.0, 6;
    %cmpi/u 8, 1, 4;
    %jmp/1 T_0.1, 6;
    %cmpi/u 8, 2, 4;
    %jmp/1 T_0.2, 6;
    %cmpi/u 8, 3, 4;
    %jmp/1 T_0.3, 6;
    %cmpi/u 8, 4, 4;
    %jmp/1 T_0.4, 6;
    %cmpi/u 8, 5, 4;
    %jmp/1 T_0.5, 6;
    %cmpi/u 8, 6, 4;
    %jmp/1 T_0.6, 6;
    %cmpi/u 8, 7, 4;
    %jmp/1 T_0.7, 6;
    %cmpi/u 8, 8, 4;
    %jmp/1 T_0.8, 6;
    %cmpi/u 8, 9, 4;
    %jmp/1 T_0.9, 6;
    %set/v v0x1b82fd0_0, 0, 7;
    %jmp T_0.11;
T_0.0 ;
    %movi 8, 126, 7;
    %set/v v0x1b82fd0_0, 8, 7;
    %jmp T_0.11;
T_0.1 ;
    %movi 8, 48, 7;
    %set/v v0x1b82fd0_0, 8, 7;
    %jmp T_0.11;
T_0.2 ;
    %movi 8, 109, 7;
    %set/v v0x1b82fd0_0, 8, 7;
    %jmp T_0.11;
T_0.3 ;
    %movi 8, 121, 7;
    %set/v v0x1b82fd0_0, 8, 7;
    %jmp T_0.11;
T_0.4 ;
    %movi 8, 51, 7;
    %set/v v0x1b82fd0_0, 8, 7;
    %jmp T_0.11;
T_0.5 ;
    %movi 8, 91, 7;
    %set/v v0x1b82fd0_0, 8, 7;
    %jmp T_0.11;
T_0.6 ;
    %movi 8, 95, 7;
    %set/v v0x1b82fd0_0, 8, 7;
    %jmp T_0.11;
T_0.7 ;
    %movi 8, 112, 7;
    %set/v v0x1b82fd0_0, 8, 7;
    %jmp T_0.11;
T_0.8 ;
    %set/v v0x1b82fd0_0, 1, 7;
    %jmp T_0.11;
T_0.9 ;
    %movi 8, 123, 7;
    %set/v v0x1b82fd0_0, 8, 7;
    %jmp T_0.11;
T_0.11 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x1b4c730;
T_1 ;
    %set/v v0x1b93e40_0, 0, 32;
    %end;
    .thread T_1;
    .scope S_0x1b4c730;
T_2 ;
    %vpi_call 2 10 "$dumpfile", "BcdTo7segmentwaveform.vcd";
    %vpi_call 2 11 "$dumpvars", 1'sb0, S_0x1b4c730;
    %end;
    .thread T_2;
    .scope S_0x1b4c730;
T_3 ;
    %vpi_call 2 16 "$monitor", $time, "  bcd = %d , Out = %b", v0x1b93d90_0, v0x1b93cc0_0;
    %set/v v0x1b93e40_0, 0, 32;
T_3.0 ;
    %load/v 8, v0x1b93e40_0, 32;
   %cmpi/s 8, 16, 32;
    %jmp/0xz T_3.1, 5;
    %load/v 8, v0x1b93e40_0, 32;
    %set/v v0x1b93d90_0, 8, 4;
    %delay 5, 0;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x1b93e40_0, 32;
    %set/v v0x1b93e40_0, 8, 32;
    %jmp T_3.0;
T_3.1 ;
    %delay 5, 0;
    %set/v v0x1b93d90_0, 3, 4;
    %delay 5, 0;
    %set/v v0x1b93d90_0, 2, 4;
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
