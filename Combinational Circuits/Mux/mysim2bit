#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1297730 .scope module, "twotoonemuxTB" "twotoonemuxTB" 2 1;
 .timescale 0 0;
v0x12e31d0_0 .net "Out", 0 0, v0x12ccf30_0; 1 drivers
v0x12e32a0_0 .var/i "i", 31 0;
v0x12e3320_0 .var "in0", 0 0;
v0x12e33d0_0 .var "in1", 0 0;
v0x12e34b0_0 .var "sel", 0 0;
S_0x12e30e0 .scope task, "initialize" "initialize" 2 19, 2 19, S_0x1297730;
 .timescale 0 0;
TD_twotoonemuxTB.initialize ;
    %set/v v0x12e3320_0, 0, 1;
    %set/v v0x12e33d0_0, 0, 1;
    %set/v v0x12e34b0_0, 0, 1;
    %set/v v0x12e32a0_0, 0, 32;
    %end;
S_0x12e2ff0 .scope task, "stimulus" "stimulus" 2 28, 2 28, S_0x1297730;
 .timescale 0 0;
TD_twotoonemuxTB.stimulus ;
    %set/v v0x12e32a0_0, 0, 32;
T_1.0 ;
    %load/v 8, v0x12e32a0_0, 32;
   %cmpi/s 8, 8, 32;
    %jmp/0xz T_1.1, 5;
    %load/v 8, v0x12e32a0_0, 32;
    %set/v v0x12e34b0_0, 8, 1;
    %set/v v0x12e3320_0, 9, 1;
    %set/v v0x12e33d0_0, 10, 1;
    %delay 5, 0;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x12e32a0_0, 32;
    %set/v v0x12e32a0_0, 8, 32;
    %jmp T_1.0;
T_1.1 ;
    %end;
S_0x1297820 .scope module, "DUT" "twotoonemux" 2 10, 3 1, S_0x1297730;
 .timescale 0 0;
v0x12ccf30_0 .var "Out", 0 0;
v0x12e2e00_0 .net "in0", 0 0, v0x12e3320_0; 1 drivers
v0x12e2ea0_0 .net "in1", 0 0, v0x12e33d0_0; 1 drivers
v0x12e2f40_0 .net "sel", 0 0, v0x12e34b0_0; 1 drivers
E_0x12a8fe0 .event edge, v0x12e2f40_0, v0x12e2ea0_0, v0x12e2e00_0;
    .scope S_0x1297820;
T_2 ;
    %wait E_0x12a8fe0;
    %load/v 8, v0x12e2f40_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_2.0, 4;
    %load/v 8, v0x12e2ea0_0, 1;
    %set/v v0x12ccf30_0, 8, 1;
    %jmp T_2.1;
T_2.0 ;
    %load/v 8, v0x12e2e00_0, 1;
    %set/v v0x12ccf30_0, 8, 1;
T_2.1 ;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x1297730;
T_3 ;
    %vpi_call 2 14 "$monitor", $time, " in = %b%b, sel = %b,  Out = %b", v0x12e33d0_0, v0x12e3320_0, v0x12e34b0_0, v0x12e31d0_0;
    %vpi_call 2 15 "$dumpfile", "twotoonemuxwaveform.vcd";
    %vpi_call 2 16 "$dumpvars", 1'sb0, S_0x1297730;
    %end;
    .thread T_3;
    .scope S_0x1297730;
T_4 ;
    %fork TD_twotoonemuxTB.initialize, S_0x12e30e0;
    %join;
    %fork TD_twotoonemuxTB.stimulus, S_0x12e2ff0;
    %join;
    %delay 5, 0;
    %set/v v0x12e33d0_0, 1, 1;
    %set/v v0x12e3320_0, 0, 1;
    %set/v v0x12e34b0_0, 0, 1;
    %delay 5, 0;
    %set/v v0x12e33d0_0, 1, 1;
    %set/v v0x12e3320_0, 0, 1;
    %set/v v0x12e34b0_0, 1, 1;
    %delay 5, 0;
    %set/v v0x12e33d0_0, 1, 1;
    %set/v v0x12e3320_0, 1, 1;
    %set/v v0x12e34b0_0, 0, 1;
    %delay 5, 0;
    %set/v v0x12e33d0_0, 1, 1;
    %set/v v0x12e3320_0, 1, 1;
    %set/v v0x12e34b0_0, 1, 1;
    %delay 5, 0;
    %set/v v0x12e33d0_0, 0, 1;
    %set/v v0x12e3320_0, 1, 1;
    %set/v v0x12e34b0_0, 0, 1;
    %delay 5, 0;
    %set/v v0x12e33d0_0, 0, 1;
    %set/v v0x12e3320_0, 1, 1;
    %set/v v0x12e34b0_0, 1, 1;
    %delay 5, 0;
    %set/v v0x12e33d0_0, 0, 1;
    %set/v v0x12e3320_0, 0, 1;
    %set/v v0x12e34b0_0, 0, 1;
    %delay 5, 0;
    %set/v v0x12e33d0_0, 0, 1;
    %set/v v0x12e3320_0, 0, 1;
    %set/v v0x12e34b0_0, 1, 1;
    %delay 5, 0;
    %vpi_call 2 50 "$finish";
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "2to1muxTB.v";
    "2to1mux.v";
