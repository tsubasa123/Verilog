#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1d94730 .scope module, "EvenParityfourbitTB" "EvenParityfourbitTB" 2 1;
 .timescale 0 0;
v0x1de3020_0 .var "A", 2 0;
v0x1de30f0_0 .net "Out", 3 0, v0x1de2d90_0; 1 drivers
v0x1de31a0_0 .net "parity_bit", 0 0, v0x1de2f80_0; 1 drivers
S_0x1d94820 .scope module, "DUT" "EvenParityfourbit" 2 7, 3 1, S_0x1d94730;
 .timescale 0 0;
v0x1dcbbd0_0 .net "A", 2 0, v0x1de3020_0; 1 drivers
v0x1de2d90_0 .var "Out", 3 0;
v0x1de2e30_0 .var/i "count", 31 0;
v0x1de2ed0_0 .var/i "i", 31 0;
v0x1de2f80_0 .var "parity_bit", 0 0;
E_0x1da7730 .event edge, v0x1dcbbd0_0;
    .scope S_0x1d94820;
T_0 ;
    %set/v v0x1de2e30_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_0x1d94820;
T_1 ;
    %wait E_0x1da7730;
    %set/v v0x1de2e30_0, 0, 32;
    %set/v v0x1de2ed0_0, 0, 32;
T_1.0 ;
    %load/v 8, v0x1de2ed0_0, 32;
   %cmpi/s 8, 2, 32;
    %or 5, 4, 1;
    %jmp/0xz T_1.1, 5;
    %ix/getv/s 1, v0x1de2ed0_0;
    %jmp/1 T_1.2, 4;
    %load/x1p 11, v0x1dcbbd0_0, 1;
    %jmp T_1.3;
T_1.2 ;
    %mov 11, 2, 1;
T_1.3 ;
    %mov 8, 11, 1; Move signal select into place
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_1.4, 4;
    %load/v 8, v0x1de2e30_0, 32;
    %mov 40, 39, 1;
    %addi 8, 1, 33;
    %set/v v0x1de2e30_0, 8, 32;
T_1.4 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x1de2ed0_0, 32;
    %set/v v0x1de2ed0_0, 8, 32;
    %jmp T_1.0;
T_1.1 ;
    %load/v 8, v0x1de2e30_0, 32;
    %movi 40, 2, 32;
    %mod/s 8, 40, 32;
    %cmpi/u 8, 0, 32;
    %jmp/0xz  T_1.6, 4;
    %set/v v0x1de2f80_0, 0, 1;
    %load/v 8, v0x1de2f80_0, 1;
    %load/v 9, v0x1dcbbd0_0, 3;
    %set/v v0x1de2d90_0, 8, 4;
    %jmp T_1.7;
T_1.6 ;
    %load/v 8, v0x1de2e30_0, 32;
    %movi 40, 2, 32;
    %mod/s 8, 40, 32;
    %cmpi/u 8, 0, 32;
    %inv 4, 1;
    %jmp/0xz  T_1.8, 4;
    %set/v v0x1de2f80_0, 1, 1;
    %load/v 8, v0x1de2f80_0, 1;
    %load/v 9, v0x1dcbbd0_0, 3;
    %set/v v0x1de2d90_0, 8, 4;
T_1.8 ;
T_1.7 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x1d94730;
T_2 ;
    %vpi_call 2 11 "$dumpfile", "EvenParitywaveform.vcd";
    %vpi_call 2 12 "$dumpvars", 1'sb0, S_0x1d94730;
    %end;
    .thread T_2;
    .scope S_0x1d94730;
T_3 ;
    %vpi_call 2 16 "$monitor", $time, " A[0] = %b, A[1] = %b, A[2] = %b, Out = %b, parity_bit  = %b", &PV<v0x1de3020_0, 0, 1>, &PV<v0x1de3020_0, 1, 1>, &PV<v0x1de3020_0, 2, 1>, v0x1de30f0_0, v0x1de31a0_0;
    %delay 0, 0;
    %ix/load 0, 2, 0;
    %set/x0 v0x1de3020_0, 0, 1;
    %ix/load 0, 1, 0;
    %set/x0 v0x1de3020_0, 0, 1;
    %ix/load 0, 0, 0;
    %set/x0 v0x1de3020_0, 0, 1;
    %delay 5, 0;
    %ix/load 0, 2, 0;
    %set/x0 v0x1de3020_0, 0, 1;
    %ix/load 0, 1, 0;
    %set/x0 v0x1de3020_0, 0, 1;
    %ix/load 0, 0, 0;
    %set/x0 v0x1de3020_0, 1, 1;
    %delay 5, 0;
    %ix/load 0, 2, 0;
    %set/x0 v0x1de3020_0, 0, 1;
    %ix/load 0, 1, 0;
    %set/x0 v0x1de3020_0, 1, 1;
    %ix/load 0, 0, 0;
    %set/x0 v0x1de3020_0, 0, 1;
    %delay 5, 0;
    %ix/load 0, 2, 0;
    %set/x0 v0x1de3020_0, 0, 1;
    %ix/load 0, 1, 0;
    %set/x0 v0x1de3020_0, 1, 1;
    %ix/load 0, 0, 0;
    %set/x0 v0x1de3020_0, 1, 1;
    %delay 5, 0;
    %ix/load 0, 2, 0;
    %set/x0 v0x1de3020_0, 1, 1;
    %ix/load 0, 1, 0;
    %set/x0 v0x1de3020_0, 0, 1;
    %ix/load 0, 0, 0;
    %set/x0 v0x1de3020_0, 0, 1;
    %delay 5, 0;
    %ix/load 0, 2, 0;
    %set/x0 v0x1de3020_0, 1, 1;
    %ix/load 0, 1, 0;
    %set/x0 v0x1de3020_0, 0, 1;
    %ix/load 0, 0, 0;
    %set/x0 v0x1de3020_0, 1, 1;
    %delay 5, 0;
    %ix/load 0, 2, 0;
    %set/x0 v0x1de3020_0, 1, 1;
    %ix/load 0, 1, 0;
    %set/x0 v0x1de3020_0, 1, 1;
    %ix/load 0, 0, 0;
    %set/x0 v0x1de3020_0, 0, 1;
    %delay 5, 0;
    %ix/load 0, 2, 0;
    %set/x0 v0x1de3020_0, 1, 1;
    %ix/load 0, 1, 0;
    %set/x0 v0x1de3020_0, 1, 1;
    %ix/load 0, 0, 0;
    %set/x0 v0x1de3020_0, 1, 1;
    %delay 5, 0;
    %vpi_call 2 25 "$finish";
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
