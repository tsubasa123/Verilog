#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1c1bf60 .scope module, "onebitadderTB" "onebitadderTB" 2 1;
 .timescale 0 0;
v0x1c30860_0 .var "A", 0 0;
v0x1c30930_0 .var "B", 0 0;
v0x1c30a00_0 .var "CarryIn", 0 0;
v0x1c30ad0_0 .net "CarryOut", 0 0, L_0x1c30c20; 1 drivers
v0x1c30b50_0 .net "Sum", 0 0, v0x1c2fc80_0; 1 drivers
S_0x1c30770 .scope task, "init" "init" 2 12, 2 12, S_0x1c1bf60;
 .timescale 0 0;
TD_onebitadderTB.init ;
    %set/v v0x1c30860_0, 0, 1;
    %set/v v0x1c30930_0, 0, 1;
    %set/v v0x1c30a00_0, 0, 1;
    %end;
S_0x1c1c050 .scope module, "DUT" "onebitadder" 2 10, 3 1, S_0x1c1bf60;
 .timescale 0 0;
L_0x1c30c20 .functor OR 1, v0x1c2fff0_0, v0x1c2fbe0_0, C4<0>, C4<0>;
v0x1c30170_0 .net "A", 0 0, v0x1c30860_0; 1 drivers
v0x1c30220_0 .net "B", 0 0, v0x1c30930_0; 1 drivers
v0x1c302d0_0 .net "CarryIn", 0 0, v0x1c30a00_0; 1 drivers
v0x1c30380_0 .alias "CarryOut", 0 0, v0x1c30ad0_0;
v0x1c30430_0 .alias "Sum", 0 0, v0x1c30b50_0;
v0x1c304e0_0 .net "c1", 0 0, v0x1c2fff0_0; 1 drivers
v0x1c305a0_0 .net "c2", 0 0, v0x1c2fbe0_0; 1 drivers
v0x1c30650_0 .net "s1", 0 0, v0x1c30090_0; 1 drivers
S_0x1c2fd30 .scope module, "ha1" "HalfAdder" 3 13, 4 1, S_0x1c1c050;
 .timescale 0 0;
v0x1c2fe90_0 .alias "A", 0 0, v0x1c30170_0;
v0x1c2ff50_0 .alias "B", 0 0, v0x1c30220_0;
v0x1c2fff0_0 .var "Carry", 0 0;
v0x1c30090_0 .var "Sum", 0 0;
E_0x1c2fe20 .event edge, v0x1c2fe90_0, v0x1c2ff50_0;
S_0x1be1690 .scope module, "ha2" "HalfAdder" 3 14, 4 1, S_0x1c1c050;
 .timescale 0 0;
v0x1bf61e0_0 .alias "A", 0 0, v0x1c30650_0;
v0x1c2fb40_0 .alias "B", 0 0, v0x1c302d0_0;
v0x1c2fbe0_0 .var "Carry", 0 0;
v0x1c2fc80_0 .var "Sum", 0 0;
E_0x1be1bc0 .event edge, v0x1bf61e0_0, v0x1c2fb40_0;
    .scope S_0x1c2fd30;
T_1 ;
    %wait E_0x1c2fe20;
    %load/v 8, v0x1c2ff50_0, 1;
    %load/v 9, v0x1c2fe90_0, 1;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_1.0, 6;
    %cmpi/u 8, 3, 2;
    %jmp/1 T_1.1, 6;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1c30090_0, 0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1c2fff0_0, 0, 0;
    %jmp T_1.3;
T_1.0 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1c30090_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1c2fff0_0, 0, 0;
    %jmp T_1.3;
T_1.1 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1c30090_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1c2fff0_0, 0, 1;
    %jmp T_1.3;
T_1.3 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x1be1690;
T_2 ;
    %wait E_0x1be1bc0;
    %load/v 8, v0x1c2fb40_0, 1;
    %load/v 9, v0x1bf61e0_0, 1;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_2.0, 6;
    %cmpi/u 8, 3, 2;
    %jmp/1 T_2.1, 6;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1c2fc80_0, 0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1c2fbe0_0, 0, 0;
    %jmp T_2.3;
T_2.0 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1c2fc80_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1c2fbe0_0, 0, 0;
    %jmp T_2.3;
T_2.1 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1c2fc80_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1c2fbe0_0, 0, 1;
    %jmp T_2.3;
T_2.3 ;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x1c1bf60;
T_3 ;
    %vpi_call 2 23 "$monitor", $time, " A = %b, B= %b, CarryIn = %b, Result = %b%b", v0x1c30860_0, v0x1c30930_0, v0x1c30a00_0, v0x1c30ad0_0, v0x1c30b50_0;
    %vpi_call 2 24 "$dumpfile", "Onebitadderwaveform.vcd";
    %vpi_call 2 25 "$dumpvars", 1'sb0, S_0x1c1bf60;
    %end;
    .thread T_3;
    .scope S_0x1c1bf60;
T_4 ;
    %fork TD_onebitadderTB.init, S_0x1c30770;
    %join;
    %delay 5, 0;
    %set/v v0x1c30860_0, 0, 1;
    %set/v v0x1c30930_0, 0, 1;
    %set/v v0x1c30a00_0, 0, 1;
    %delay 5, 0;
    %set/v v0x1c30860_0, 0, 1;
    %set/v v0x1c30930_0, 0, 1;
    %set/v v0x1c30a00_0, 1, 1;
    %delay 5, 0;
    %set/v v0x1c30860_0, 0, 1;
    %set/v v0x1c30930_0, 1, 1;
    %set/v v0x1c30a00_0, 0, 1;
    %delay 5, 0;
    %set/v v0x1c30860_0, 0, 1;
    %set/v v0x1c30930_0, 1, 1;
    %set/v v0x1c30a00_0, 1, 1;
    %delay 5, 0;
    %set/v v0x1c30860_0, 1, 1;
    %set/v v0x1c30930_0, 0, 1;
    %set/v v0x1c30a00_0, 0, 1;
    %delay 5, 0;
    %set/v v0x1c30860_0, 1, 1;
    %set/v v0x1c30930_0, 0, 1;
    %set/v v0x1c30a00_0, 1, 1;
    %delay 5, 0;
    %set/v v0x1c30860_0, 1, 1;
    %set/v v0x1c30930_0, 1, 1;
    %set/v v0x1c30a00_0, 0, 1;
    %delay 5, 0;
    %set/v v0x1c30860_0, 1, 1;
    %set/v v0x1c30930_0, 1, 1;
    %set/v v0x1c30a00_0, 1, 1;
    %delay 5, 0;
    %vpi_call 2 40 "$finish";
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "1bitadderTB.v";
    "1bitadder.v";
    "../HalfAdder/HalfAdder.v";
