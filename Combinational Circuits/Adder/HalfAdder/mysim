#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x15ba730 .scope module, "HalfAdderTB" "HalfAdderTB" 2 1;
 .timescale 0 0;
v0x16022c0_0 .var "A", 0 0;
v0x1602390_0 .var "B", 0 0;
v0x1602440_0 .net "Carry", 0 0, v0x1602080_0; 1 drivers
v0x16024f0_0 .net "Sum", 0 0, v0x1602120_0; 1 drivers
S_0x16021d0 .scope task, "Init" "Init" 2 9, 2 9, S_0x15ba730;
 .timescale 0 0;
TD_HalfAdderTB.Init ;
    %set/v v0x16022c0_0, 0, 1;
    %set/v v0x1602390_0, 0, 1;
    %end;
S_0x15ba820 .scope module, "DUT" "HalfAdder" 2 7, 3 1, S_0x15ba730;
 .timescale 0 0;
v0x15f0800_0 .net "A", 0 0, v0x16022c0_0; 1 drivers
v0x1601fe0_0 .net "B", 0 0, v0x1602390_0; 1 drivers
v0x1602080_0 .var "Carry", 0 0;
v0x1602120_0 .var "Sum", 0 0;
E_0x15cce20 .event edge, v0x15f0800_0, v0x1601fe0_0;
    .scope S_0x15ba820;
T_1 ;
    %wait E_0x15cce20;
    %load/v 8, v0x1601fe0_0, 1;
    %load/v 9, v0x15f0800_0, 1;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_1.0, 6;
    %cmpi/u 8, 3, 2;
    %jmp/1 T_1.1, 6;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1602120_0, 0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1602080_0, 0, 0;
    %jmp T_1.3;
T_1.0 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1602120_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1602080_0, 0, 0;
    %jmp T_1.3;
T_1.1 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1602120_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1602080_0, 0, 1;
    %jmp T_1.3;
T_1.3 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x15ba730;
T_2 ;
    %vpi_call 2 18 "$monitor", "A = %b , B = %b , Sum = %b, Carry = %b", v0x16022c0_0, v0x1602390_0, v0x16024f0_0, v0x1602440_0;
    %vpi_call 2 19 "$dumpfile", "HalfAdderwaveform.vcd";
    %vpi_call 2 20 "$dumpvars", 1'sb0, S_0x15ba730;
    %end;
    .thread T_2;
    .scope S_0x15ba730;
T_3 ;
    %fork TD_HalfAdderTB.Init, S_0x16021d0;
    %join;
    %delay 10, 0;
    %set/v v0x16022c0_0, 0, 1;
    %set/v v0x1602390_0, 0, 1;
    %delay 5, 0;
    %set/v v0x16022c0_0, 0, 1;
    %set/v v0x1602390_0, 1, 1;
    %delay 5, 0;
    %set/v v0x16022c0_0, 1, 1;
    %set/v v0x1602390_0, 0, 1;
    %delay 5, 0;
    %set/v v0x16022c0_0, 1, 1;
    %set/v v0x1602390_0, 1, 1;
    %delay 5, 0;
    %vpi_call 2 30 "$finish";
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "HalfAdderTB.v";
    "HalfAdder.v";
