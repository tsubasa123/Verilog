#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x22ae690 .scope module, "OrGateTB" "OrGateTB" 2 1;
 .timescale 0 0;
v0x22f3120_0 .var "A", 0 0;
v0x22f31f0_0 .var "B", 0 0;
v0x22f32a0_0 .net "Out", 0 0, L_0x22f3350; 1 drivers
S_0x22ae780 .scope module, "DUT" "OrGate" 2 5, 3 1, S_0x22ae690;
 .timescale 0 0;
L_0x22f3350 .functor OR 1, v0x22f3120_0, v0x22f31f0_0, C4<0>, C4<0>;
v0x22aded0_0 .net "A", 0 0, v0x22f3120_0; 1 drivers
v0x22f2fe0_0 .net "B", 0 0, v0x22f31f0_0; 1 drivers
v0x22f3080_0 .alias "Out", 0 0, v0x22f32a0_0;
    .scope S_0x22ae690;
T_0 ;
    %vpi_call 2 10 "$dumpfile", "Orwaveform.vcd";
    %vpi_call 2 11 "$dumpvars", 1'sb0, S_0x22ae690;
    %end;
    .thread T_0;
    .scope S_0x22ae690;
T_1 ;
    %vpi_call 2 16 "$monitor", $time, " A = %b, B = %b , Out = %b", v0x22f3120_0, v0x22f31f0_0, v0x22f32a0_0;
    %set/v v0x22f3120_0, 0, 1;
    %set/v v0x22f31f0_0, 0, 1;
    %delay 5, 0;
    %set/v v0x22f3120_0, 0, 1;
    %set/v v0x22f31f0_0, 1, 1;
    %delay 5, 0;
    %set/v v0x22f3120_0, 1, 1;
    %set/v v0x22f31f0_0, 0, 1;
    %delay 5, 0;
    %set/v v0x22f3120_0, 0, 1;
    %set/v v0x22f31f0_0, 1, 1;
    %delay 5, 0;
    %vpi_call 2 21 "$finish";
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "OrGateTB.v";
    "OrGate.v";
