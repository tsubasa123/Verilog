#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xa0d690 .scope module, "AndTB" "AndTB" 2 1;
 .timescale 0 0;
v0xa56e10_0 .var "A", 0 0;
v0xa56ee0_0 .var "B", 0 0;
v0xa56f90_0 .var "C", 0 0;
v0xa57040_0 .net "Out", 0 0, L_0xa57250; 1 drivers
S_0xa0d780 .scope module, "DUT" "And4inp" 2 5, 3 1, S_0xa0d690;
 .timescale 0 0;
L_0xa57120 .functor AND 1, v0xa56e10_0, v0xa56ee0_0, C4<1>, C4<1>;
L_0xa57250 .functor AND 1, L_0xa57120, v0xa56f90_0, C4<1>, C4<1>;
v0xa1ed10_0 .net "A", 0 0, v0xa56e10_0; 1 drivers
v0xa56b80_0 .net "B", 0 0, v0xa56ee0_0; 1 drivers
v0xa56c20_0 .net "C", 0 0, v0xa56f90_0; 1 drivers
v0xa56cc0_0 .alias "Out", 0 0, v0xa57040_0;
v0xa56d70_0 .net *"_s0", 0 0, L_0xa57120; 1 drivers
    .scope S_0xa0d690;
T_0 ;
    %vpi_call 2 9 "$dumpfile", "Andwaveform.vcd";
    %vpi_call 2 10 "$dumpvars", 1'sb0, S_0xa0d690;
    %end;
    .thread T_0;
    .scope S_0xa0d690;
T_1 ;
    %vpi_call 2 15 "$monitor", $time, " A = %b, B = %b , C = %b, Out = %b", v0xa56e10_0, v0xa56ee0_0, v0xa56f90_0, v0xa57040_0;
    %delay 5, 0;
    %set/v v0xa56e10_0, 0, 1;
    %set/v v0xa56ee0_0, 0, 1;
    %set/v v0xa56f90_0, 0, 1;
    %delay 5, 0;
    %set/v v0xa56e10_0, 0, 1;
    %set/v v0xa56ee0_0, 1, 1;
    %set/v v0xa56f90_0, 0, 1;
    %delay 5, 0;
    %set/v v0xa56e10_0, 1, 1;
    %set/v v0xa56ee0_0, 1, 1;
    %set/v v0xa56f90_0, 1, 1;
    %delay 5, 0;
    %set/v v0xa56e10_0, 1, 1;
    %set/v v0xa56ee0_0, 0, 1;
    %set/v v0xa56f90_0, 0, 1;
    %delay 5, 0;
    %set/v v0xa56e10_0, 1, 1;
    %set/v v0xa56ee0_0, 0, 1;
    %set/v v0xa56f90_0, 1, 1;
    %delay 5, 0;
    %set/v v0xa56e10_0, 1, 1;
    %set/v v0xa56ee0_0, 1, 1;
    %set/v v0xa56f90_0, 1, 1;
    %delay 5, 0;
    %set/v v0xa56e10_0, 0, 1;
    %set/v v0xa56ee0_0, 1, 1;
    %set/v v0xa56f90_0, 0, 1;
    %delay 5, 0;
    %set/v v0xa56e10_0, 0, 1;
    %set/v v0xa56ee0_0, 0, 1;
    %set/v v0xa56f90_0, 1, 1;
    %delay 5, 0;
    %set/v v0xa56e10_0, 1, 1;
    %set/v v0xa56ee0_0, 0, 1;
    %set/v v0xa56f90_0, 1, 1;
    %delay 5, 0;
    %set/v v0xa56e10_0, 1, 1;
    %set/v v0xa56ee0_0, 1, 1;
    %set/v v0xa56f90_0, 1, 1;
