#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xd6b690 .scope module, "AndGateTB" "AndGateTB" 2 1;
 .timescale 0 0;
v0xdafab0_0 .var "A", 0 0;
v0xdafb80_0 .var "B", 0 0;
v0xdafc30_0 .net "Out", 0 0, L_0xdafce0; 1 drivers
S_0xd6b780 .scope module, "DUT" "AndGate" 2 5, 3 1, S_0xd6b690;
 .timescale 0 0;
L_0xdafce0 .functor AND 1, v0xdafab0_0, v0xdafb80_0, C4<1>, C4<1>;
v0xd6ae90_0 .net "A", 0 0, v0xdafab0_0; 1 drivers
v0xdaf970_0 .net "B", 0 0, v0xdafb80_0; 1 drivers
v0xdafa10_0 .alias "Out", 0 0, v0xdafc30_0;
    .scope S_0xd6b690;
T_0 ;
    %vpi_call 2 11 "$monitor", $time, "A = %b , B = %b, Out = %b", v0xdafab0_0, v0xdafb80_0, v0xdafc30_0;
    %set/v v0xdafab0_0, 0, 1;
    %set/v v0xdafb80_0, 0, 1;
    %delay 5, 0;
    %set/v v0xdafab0_0, 0, 1;
    %set/v v0xdafb80_0, 1, 1;
    %delay 5, 0;
    %set/v v0xdafab0_0, 1, 1;
    %set/v v0xdafb80_0, 1, 1;
    %delay 5, 0;
    %vpi_call 2 15 "$finish";
    %end;
    .thread T_0;
    .scope S_0xd6b690;
T_1 ;
    %vpi_call 2 20 "$dumpfile", "andwaveform.vcd";
    %vpi_call 2 21 "$dumpvars", 1'sb0, S_0xd6b690;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "AndGateTB.v";
    "AndGate.v";
