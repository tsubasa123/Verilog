#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1e52730 .scope module, "XorGateTB" "XorGateTB" 2 1;
 .timescale 0 0;
v0x1e99e60_0 .var "A", 0 0;
v0x1e99f30_0 .var "B", 0 0;
v0x1e99fe0_0 .net "Out", 0 0, v0x1e99dc0_0; 1 drivers
S_0x1e52820 .scope module, "DUT" "XorGate" 2 6, 3 1, S_0x1e52730;
 .timescale 0 0;
v0x1e88460_0 .net "A", 0 0, v0x1e99e60_0; 1 drivers
v0x1e99d20_0 .net "B", 0 0, v0x1e99f30_0; 1 drivers
v0x1e99dc0_0 .var "Out", 0 0;
E_0x1e64240 .event edge, v0x1e88460_0, v0x1e99d20_0;
    .scope S_0x1e52820;
T_0 ;
    %wait E_0x1e64240;
    %load/v 8, v0x1e88460_0, 1;
    %cmpi/u 8, 0, 1;
    %mov 8, 4, 1;
    %load/v 9, v0x1e99d20_0, 1;
    %cmpi/u 9, 0, 1;
    %mov 9, 4, 1;
    %and 8, 9, 1;
    %jmp/0xz  T_0.0, 8;
    %set/v v0x1e99dc0_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x1e88460_0, 1;
    %load/v 9, v0x1e99d20_0, 1;
    %and 8, 9, 1;
    %jmp/0xz  T_0.2, 8;
    %set/v v0x1e99dc0_0, 0, 1;
    %jmp T_0.3;
T_0.2 ;
    %set/v v0x1e99dc0_0, 1, 1;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x1e52730;
T_1 ;
    %vpi_call 2 10 "$dumpfile", "Xorwaveform.vcd";
    %vpi_call 2 11 "$dumpvars", 1'sb0, S_0x1e52730;
    %end;
    .thread T_1;
    .scope S_0x1e52730;
T_2 ;
    %vpi_call 2 16 "$monitor", $time, " A = %b, B = %b, Out = %b", v0x1e99e60_0, v0x1e99f30_0, v0x1e99fe0_0;
    %delay 0, 0;
    %set/v v0x1e99e60_0, 0, 1;
    %set/v v0x1e99f30_0, 0, 1;
    %delay 5, 0;
    %set/v v0x1e99e60_0, 0, 1;
    %set/v v0x1e99f30_0, 1, 1;
    %delay 5, 0;
    %set/v v0x1e99e60_0, 1, 1;
    %set/v v0x1e99f30_0, 0, 1;
    %delay 5, 0;
    %set/v v0x1e99e60_0, 1, 1;
    %set/v v0x1e99f30_0, 1, 1;
    %delay 5, 0;
    %set/v v0x1e99e60_0, 0, 1;
    %set/v v0x1e99f30_0, 0, 1;
    %delay 5, 0;
    %set/v v0x1e99e60_0, 0, 1;
    %set/v v0x1e99f30_0, 1, 1;
    %delay 5, 0;
    %set/v v0x1e99e60_0, 0, 1;
    %set/v v0x1e99f30_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 24 "$finish";
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "XorGateTB.v";
    "XorGate.v";
