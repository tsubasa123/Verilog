#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x25b5760 .scope module, "eighttothreeencoderTB" "eighttothreeencoderTB" 2 1;
 .timescale 0 0;
v0x25fec40_0 .net "code", 2 0, v0x25ec4f0_0; 1 drivers
v0x25fed10_0 .var "din", 7 0;
S_0x25feb50 .scope task, "initialize" "initialize" 2 16, 2 16, S_0x25b5760;
 .timescale 0 0;
TD_eighttothreeencoderTB.initialize ;
    %set/v v0x25fed10_0, 0, 8;
    %end;
S_0x25ec400 .scope module, "DUT" "eighttothreeencoder" 2 7, 3 1, S_0x25b5760;
 .timescale 0 0;
v0x25ec4f0_0 .var "code", 2 0;
v0x25feab0_0 .net "din", 7 0, v0x25fed10_0; 1 drivers
E_0x25b5690 .event edge, v0x25feab0_0;
    .scope S_0x25ec400;
T_1 ;
    %wait E_0x25b5690;
    %load/v 8, v0x25feab0_0, 1; Select 1 out of 8 bits
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_1.0, 4;
    %set/v v0x25ec4f0_0, 0, 3;
    %jmp T_1.1;
T_1.0 ;
    %ix/load 1, 1, 0;
    %mov 4, 0, 1;
    %jmp/1 T_1.2, 4;
    %load/x1p 11, v0x25feab0_0, 1;
    %jmp T_1.3;
T_1.2 ;
    %mov 11, 2, 1;
T_1.3 ;
    %mov 8, 11, 1; Move signal select into place
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_1.4, 4;
    %movi 8, 1, 3;
    %set/v v0x25ec4f0_0, 8, 3;
    %jmp T_1.5;
T_1.4 ;
    %ix/load 1, 2, 0;
    %mov 4, 0, 1;
    %jmp/1 T_1.6, 4;
    %load/x1p 11, v0x25feab0_0, 1;
    %jmp T_1.7;
T_1.6 ;
    %mov 11, 2, 1;
T_1.7 ;
    %mov 8, 11, 1; Move signal select into place
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_1.8, 4;
    %movi 8, 2, 3;
    %set/v v0x25ec4f0_0, 8, 3;
    %jmp T_1.9;
T_1.8 ;
    %ix/load 1, 3, 0;
    %mov 4, 0, 1;
    %jmp/1 T_1.10, 4;
    %load/x1p 11, v0x25feab0_0, 1;
    %jmp T_1.11;
T_1.10 ;
    %mov 11, 2, 1;
T_1.11 ;
    %mov 8, 11, 1; Move signal select into place
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_1.12, 4;
    %movi 8, 3, 3;
    %set/v v0x25ec4f0_0, 8, 3;
    %jmp T_1.13;
T_1.12 ;
    %ix/load 1, 4, 0;
    %mov 4, 0, 1;
    %jmp/1 T_1.14, 4;
    %load/x1p 11, v0x25feab0_0, 1;
    %jmp T_1.15;
T_1.14 ;
    %mov 11, 2, 1;
T_1.15 ;
    %mov 8, 11, 1; Move signal select into place
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_1.16, 4;
    %movi 8, 4, 3;
    %set/v v0x25ec4f0_0, 8, 3;
    %jmp T_1.17;
T_1.16 ;
    %ix/load 1, 5, 0;
    %mov 4, 0, 1;
    %jmp/1 T_1.18, 4;
    %load/x1p 11, v0x25feab0_0, 1;
    %jmp T_1.19;
T_1.18 ;
    %mov 11, 2, 1;
T_1.19 ;
    %mov 8, 11, 1; Move signal select into place
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_1.20, 4;
    %movi 8, 5, 3;
    %set/v v0x25ec4f0_0, 8, 3;
    %jmp T_1.21;
T_1.20 ;
    %ix/load 1, 6, 0;
    %mov 4, 0, 1;
    %jmp/1 T_1.22, 4;
    %load/x1p 11, v0x25feab0_0, 1;
    %jmp T_1.23;
T_1.22 ;
    %mov 11, 2, 1;
T_1.23 ;
    %mov 8, 11, 1; Move signal select into place
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_1.24, 4;
    %movi 8, 6, 3;
    %set/v v0x25ec4f0_0, 8, 3;
    %jmp T_1.25;
T_1.24 ;
    %ix/load 1, 7, 0;
    %mov 4, 0, 1;
    %jmp/1 T_1.26, 4;
    %load/x1p 11, v0x25feab0_0, 1;
    %jmp T_1.27;
T_1.26 ;
    %mov 11, 2, 1;
T_1.27 ;
    %mov 8, 11, 1; Move signal select into place
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_1.28, 4;
    %set/v v0x25ec4f0_0, 1, 3;
    %jmp T_1.29;
T_1.28 ;
    %set/v v0x25ec4f0_0, 2, 3;
T_1.29 ;
T_1.25 ;
T_1.21 ;
T_1.17 ;
T_1.13 ;
T_1.9 ;
T_1.5 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x25b5760;
T_2 ;
    %vpi_call 2 11 "$monitor", $time, " din = %b, code =%b", v0x25fed10_0, v0x25fec40_0;
    %vpi_call 2 12 "$dumpfile", "eighttothreeencoder.vcd";
    %vpi_call 2 13 "$dumpvars", 1'sb0, S_0x25b5760;
    %end;
    .thread T_2;
    .scope S_0x25b5760;
T_3 ;
    %fork TD_eighttothreeencoderTB.initialize, S_0x25feb50;
    %join;
    %delay 5, 0;
    %set/v v0x25fed10_0, 1, 8;
    %delay 5, 0;
    %movi 8, 240, 8;
    %set/v v0x25fed10_0, 8, 8;
    %delay 5, 0;
    %movi 8, 15, 8;
    %set/v v0x25fed10_0, 8, 8;
    %delay 5, 0;
    %movi 8, 68, 8;
    %set/v v0x25fed10_0, 8, 8;
    %delay 5, 0;
    %movi 8, 128, 8;
    %set/v v0x25fed10_0, 8, 8;
    %delay 5, 0;
    %movi 8, 136, 8;
    %set/v v0x25fed10_0, 8, 8;
    %delay 5, 0;
    %set/v v0x25fed10_0, 0, 8;
    %delay 5, 0;
    %movi 8, 1, 8;
    %set/v v0x25fed10_0, 8, 8;
    %delay 5, 0;
    %vpi_call 2 33 "$finish";
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "8to3priorityTB.v";
    "8to3priority.v";
