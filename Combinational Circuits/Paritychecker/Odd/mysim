#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x15d1750 .scope module, "OddParityCheckTB" "OddParityCheckTB" 2 1;
 .timescale 0 0;
v0x15e5250_0 .var "A", 3 0;
v0x15e5320_0 .net "ParityCheck", 0 0, v0x15e5060_0; 1 drivers
S_0x15d1840 .scope module, "DUT" "OddParityCheck" 2 6, 3 1, S_0x15d1750;
 .timescale 0 0;
v0x159a690_0 .net "A", 3 0, v0x15e5250_0; 1 drivers
v0x15e5060_0 .var "ParityCheck", 0 0;
v0x15e5100_0 .var/i "count", 31 0;
v0x15e51a0_0 .var/i "i", 31 0;
E_0x15acf50 .event edge, v0x159a690_0;
    .scope S_0x15d1840;
T_0 ;
    %set/v v0x15e5100_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_0x15d1840;
T_1 ;
    %set/v v0x15e51a0_0, 0, 32;
    %end;
    .thread T_1;
    .scope S_0x15d1840;
T_2 ;
    %wait E_0x15acf50;
    %set/v v0x15e5100_0, 0, 32;
    %set/v v0x15e51a0_0, 0, 32;
T_2.0 ;
    %load/v 8, v0x15e51a0_0, 32;
   %cmpi/s 8, 4, 32;
    %jmp/0xz T_2.1, 5;
    %ix/getv/s 1, v0x15e51a0_0;
    %jmp/1 T_2.2, 4;
    %load/x1p 11, v0x159a690_0, 1;
    %jmp T_2.3;
T_2.2 ;
    %mov 11, 2, 1;
T_2.3 ;
    %mov 8, 11, 1; Move signal select into place
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_2.4, 4;
    %load/v 8, v0x15e5100_0, 32;
    %mov 40, 39, 1;
    %addi 8, 1, 33;
    %set/v v0x15e5100_0, 8, 32;
    %jmp T_2.5;
T_2.4 ;
    %load/v 8, v0x15e5100_0, 32;
    %set/v v0x15e5100_0, 8, 32;
T_2.5 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x15e51a0_0, 32;
    %set/v v0x15e51a0_0, 8, 32;
    %jmp T_2.0;
T_2.1 ;
    %load/v 8, v0x15e5100_0, 32;
    %movi 40, 2, 32;
    %mod/s 8, 40, 32;
    %cmpi/u 8, 0, 32;
    %jmp/0xz  T_2.6, 4;
    %set/v v0x15e5060_0, 1, 1;
    %jmp T_2.7;
T_2.6 ;
    %set/v v0x15e5060_0, 0, 1;
T_2.7 ;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x15d1750;
T_3 ;
    %vpi_call 2 10 "$dumpfile", "OddParityCheckwaveform.vcd";
    %vpi_call 2 11 "$dumpvars", 1'sb0, S_0x15d1750;
    %end;
    .thread T_3;
    .scope S_0x15d1750;
T_4 ;
    %vpi_call 2 16 "$monitor", $time, " A = %b , ParityCheck = %b ", v0x15e5250_0, v0x15e5320_0;
    %delay 0, 0;
    %set/v v0x15e5250_0, 0, 4;
    %delay 5, 0;
    %movi 8, 1, 4;
    %set/v v0x15e5250_0, 8, 4;
    %delay 5, 0;
    %movi 8, 2, 4;
    %set/v v0x15e5250_0, 8, 4;
    %delay 5, 0;
    %movi 8, 3, 4;
    %set/v v0x15e5250_0, 8, 4;
    %delay 5, 0;
    %movi 8, 4, 4;
    %set/v v0x15e5250_0, 8, 4;
    %delay 5, 0;
    %movi 8, 5, 4;
    %set/v v0x15e5250_0, 8, 4;
    %delay 5, 0;
    %movi 8, 6, 4;
    %set/v v0x15e5250_0, 8, 4;
    %delay 5, 0;
    %movi 8, 7, 4;
    %set/v v0x15e5250_0, 8, 4;
    %delay 5, 0;
    %movi 8, 8, 4;
    %set/v v0x15e5250_0, 8, 4;
    %delay 5, 0;
    %movi 8, 9, 4;
    %set/v v0x15e5250_0, 8, 4;
    %delay 5, 0;
    %movi 8, 10, 4;
    %set/v v0x15e5250_0, 8, 4;
    %delay 5, 0;
    %movi 8, 11, 4;
    %set/v v0x15e5250_0, 8, 4;
    %delay 5, 0;
    %movi 8, 12, 4;
    %set/v v0x15e5250_0, 8, 4;
    %delay 5, 0;
    %movi 8, 13, 4;
    %set/v v0x15e5250_0, 8, 4;
    %delay 5, 0;
    %movi 8, 14, 4;
    %set/v v0x15e5250_0, 8, 4;
    %delay 5, 0;
    %set/v v0x15e5250_0, 1, 4;
    %delay 5, 0;
    %vpi_call 2 33 "$finish";
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "OddParityCheckTB.v";
