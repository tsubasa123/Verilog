#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1ef1730 .scope module, "onecountTB" "onecountTB" 2 1;
 .timescale 0 0;
v0x1f38960_0 .var "DATA", 15 0;
v0x1f38a30_0 .net "count", 4 0, v0x1f38730_0; 1 drivers
S_0x1f38870 .scope task, "initialize" "initialize" 2 15, 2 15, S_0x1ef1730;
 .timescale 0 0;
TD_onecountTB.initialize ;
    %set/v v0x1f38960_0, 0, 16;
    %end;
S_0x1ef1820 .scope module, "DUT" "onecount" 2 6, 3 1, S_0x1ef1730;
 .timescale 0 0;
v0x1f275b0_0 .net "DATA", 15 0, v0x1f38960_0; 1 drivers
v0x1f38730_0 .var "count", 4 0;
v0x1f387d0_0 .var/i "i", 31 0;
E_0x1f03db0 .event edge, v0x1f387d0_0, v0x1f275b0_0, v0x1f38730_0;
    .scope S_0x1ef1820;
T_1 ;
    %wait E_0x1f03db0;
    %set/v v0x1f38730_0, 0, 5;
    %set/v v0x1f387d0_0, 0, 32;
T_1.0 ;
    %load/v 8, v0x1f387d0_0, 32;
   %cmpi/s 8, 16, 32;
    %jmp/0xz T_1.1, 5;
    %ix/getv/s 1, v0x1f387d0_0;
    %jmp/1 T_1.2, 4;
    %load/x1p 11, v0x1f275b0_0, 1;
    %jmp T_1.3;
T_1.2 ;
    %mov 11, 2, 1;
T_1.3 ;
    %mov 8, 11, 1; Move signal select into place
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_1.4, 4;
    %load/v 8, v0x1f38730_0, 5;
    %mov 13, 0, 27;
    %addi 8, 1, 32;
    %set/v v0x1f38730_0, 8, 5;
T_1.4 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x1f387d0_0, 32;
    %set/v v0x1f387d0_0, 8, 32;
    %jmp T_1.0;
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x1ef1730;
T_2 ;
    %vpi_call 2 10 "$monitor", $time, " DATA = %b, count = %d", v0x1f38960_0, v0x1f38a30_0;
    %vpi_call 2 11 "$dumpfile", "onecountwaveform.vcd";
    %vpi_call 2 12 "$dumpvars", 1'sb0, S_0x1ef1730;
    %end;
    .thread T_2;
    .scope S_0x1ef1730;
T_3 ;
    %fork TD_onecountTB.initialize, S_0x1f38870;
    %join;
    %delay 5, 0;
    %set/v v0x1f38960_0, 1, 16;
    %delay 5, 0;
    %set/v v0x1f38960_0, 0, 16;
    %delay 5, 0;
    %movi 8, 65504, 16;
    %set/v v0x1f38960_0, 8, 16;
    %delay 5, 0;
    %movi 8, 49167, 16;
    %set/v v0x1f38960_0, 8, 16;
    %delay 5, 0;
    %movi 8, 21845, 16;
    %set/v v0x1f38960_0, 8, 16;
    %delay 5, 0;
    %movi 8, 7, 16;
    %set/v v0x1f38960_0, 8, 16;
    %delay 5, 0;
    %vpi_call 2 30 "$finish";
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "onecountTB.v";
    "onecount.v";
