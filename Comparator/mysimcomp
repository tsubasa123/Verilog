#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x218d730 .scope module, "eightbitcompTB" "eightbitcompTB" 2 1;
 .timescale 0 0;
v0x21d7630_0 .var "A", 7 0;
v0x21d7700_0 .net "A_equal_B", 0 0, v0x21d71c0_0; 1 drivers
v0x21d77b0_0 .net "A_great_B", 0 0, v0x21d7260_0; 1 drivers
v0x21d7860_0 .net "A_less_B", 0 0, v0x21d7300_0; 1 drivers
v0x21d7940_0 .var "B", 7 0;
v0x21d79f0_0 .var/i "i", 31 0;
S_0x21d7540 .scope task, "initialize" "initialize" 2 19, 2 19, S_0x218d730;
 .timescale 0 0;
TD_eightbitcompTB.initialize ;
    %set/v v0x21d7630_0, 0, 8;
    %set/v v0x21d7940_0, 0, 8;
    %end;
S_0x21d7450 .scope task, "stimulus" "stimulus" 2 26, 2 26, S_0x218d730;
 .timescale 0 0;
TD_eightbitcompTB.stimulus ;
    %set/v v0x21d79f0_0, 0, 32;
T_1.0 ;
    %load/v 8, v0x21d79f0_0, 32;
   %cmpi/s 8, 10, 32;
    %jmp/0xz T_1.1, 5;
    %delay 5, 0;
    %vpi_func 2 31 "$random", 8, 32;
    %set/v v0x21d7630_0, 8, 8;
    %vpi_func 2 32 "$random", 8, 32;
    %set/v v0x21d7940_0, 8, 8;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x21d79f0_0, 32;
    %set/v v0x21d79f0_0, 8, 32;
    %jmp T_1.0;
T_1.1 ;
    %end;
S_0x218d820 .scope module, "DUT" "eightbitcomp" 2 10, 3 1, S_0x218d730;
 .timescale 0 0;
v0x21c4690_0 .net "A", 7 0, v0x21d7630_0; 1 drivers
v0x21d71c0_0 .var "A_equal_B", 0 0;
v0x21d7260_0 .var "A_great_B", 0 0;
v0x21d7300_0 .var "A_less_B", 0 0;
v0x21d73b0_0 .net "B", 7 0, v0x21d7940_0; 1 drivers
E_0x219f210 .event edge, v0x21c4690_0, v0x21d73b0_0;
    .scope S_0x218d820;
T_2 ;
    %wait E_0x219f210;
    %load/v 8, v0x21d73b0_0, 8;
    %load/v 16, v0x21c4690_0, 8;
    %cmp/u 8, 16, 8;
    %jmp/0xz  T_2.0, 5;
    %set/v v0x21d7260_0, 1, 1;
    %set/v v0x21d71c0_0, 0, 1;
    %set/v v0x21d7300_0, 0, 1;
    %jmp T_2.1;
T_2.0 ;
    %load/v 8, v0x21c4690_0, 8;
    %load/v 16, v0x21d73b0_0, 8;
    %cmp/u 8, 16, 8;
    %jmp/0xz  T_2.2, 4;
    %set/v v0x21d7260_0, 0, 1;
    %set/v v0x21d71c0_0, 1, 1;
    %set/v v0x21d7300_0, 0, 1;
    %jmp T_2.3;
T_2.2 ;
    %load/v 8, v0x21c4690_0, 8;
    %load/v 16, v0x21d73b0_0, 8;
    %cmp/u 8, 16, 8;
    %jmp/0xz  T_2.4, 5;
    %set/v v0x21d7260_0, 0, 1;
    %set/v v0x21d71c0_0, 0, 1;
    %set/v v0x21d7300_0, 1, 1;
    %jmp T_2.5;
T_2.4 ;
    %set/v v0x21d7260_0, 0, 1;
    %set/v v0x21d71c0_0, 0, 1;
    %set/v v0x21d7300_0, 0, 1;
T_2.5 ;
T_2.3 ;
T_2.1 ;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x218d730;
T_3 ;
    %vpi_call 2 14 "$monitor", $time, " A = %d, B = %d, A_great_B = %b, A_ equal_B = %b, A_less_B =%b", v0x21d7630_0, v0x21d7940_0, v0x21d77b0_0, v0x21d7700_0, v0x21d7860_0;
    %vpi_call 2 15 "$dumpfile", "eightbitcompwaveform.vcd";
    %vpi_call 2 16 "$dumpvars", 1'sb0, S_0x218d730;
    %end;
    .thread T_3;
    .scope S_0x218d730;
T_4 ;
    %fork TD_eightbitcompTB.initialize, S_0x21d7540;
    %join;
    %fork TD_eightbitcompTB.stimulus, S_0x21d7450;
    %join;
    %delay 5, 0;
    %vpi_call 2 41 "$finish";
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "8bitcomparatorTB.v";
    "8bitcomparator.v";
